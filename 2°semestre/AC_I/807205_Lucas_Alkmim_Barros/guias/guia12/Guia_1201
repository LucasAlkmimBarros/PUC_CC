T_0.0 ;
    %load/vec4 v0x564ae6ec3db0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x564ae6ec3be0_0;
    %assign/vec4 v0x564ae6ec3e70_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x564ae6eaeab0;
T_1 ;
    %wait E_0x564ae6e73a30;
    %load/vec4 v0x564ae6ec3db0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x564ae6ec3e70_0;
    %assign/vec4 v0x564ae6ec3cc0_0, 0;
    %jmp T_1.1;
T_1.0 ;
