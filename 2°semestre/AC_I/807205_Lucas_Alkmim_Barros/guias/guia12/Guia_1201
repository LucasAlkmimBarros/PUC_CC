    .scope S_0x564ae6eae920;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564ae6ec4100_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x564ae6ec4100_0;
    %inv;
    %store/vec4 v0x564ae6ec4100_0, 0, 1;
    %jmp T_2.0;
    %end;
